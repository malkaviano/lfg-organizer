{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/group/(.*)$": "<rootDir>/src/group-organizer/$1",
    "^@/dungeon/(.*)$": "<rootDir>/src/dungeon/$1"
  }
}